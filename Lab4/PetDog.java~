import java.util.Scanner;

public class PetDog extends
{
  protected String owner;
  protected String address;
  public Petog ()
  {
 
  }
  
  public Petdog (String abreed,int aweight,char asex,int month,int day,int year,String anowner,String anaddress)
  {
     super(abread,aweight,asex,month,day,year)
     owner = anowner;
     address = anaddress;
  }
  
}